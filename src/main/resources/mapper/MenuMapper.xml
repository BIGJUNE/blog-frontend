<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blog.frontend.module.menu.dao.IMenuDao">

    <select id="listMenu" resultType="MenuPO">
        SELECT id, name, parent_id, active, url FROM menu where active = 1;
    </select>

</mapper>